{
    "env": {
        "es6": true,
        "node": true,
        "browser": false
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "rules": {
        "indent": "off",
        "semi": "off",
        "quotes": ["error", "single"]
    }
}